<launch>
    <arg name="config" value="$(find apc_vision)/config.yaml"/>
    <include file="$(find softkinetic_launch)/launch/creative_senz3d.launch">
        <arg name="rgb_resolution" value="WXGA_H"/>
    </include>
    <node name="test_tf" pkg="tf" type="static_transform_publisher"
          args=".47 -.135 .13 0 0 -1.57059632679 /shelf /senz3d_depth_optical_frame 100"/>
    <node name="vision" pkg="apc_vision" type="apc_vision_node" cwd="node" args="$(arg config)"/>
</launch>
